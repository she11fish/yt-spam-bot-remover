{
    "name": "Youtube Spam/Bot Remover",
    "version": "1.0",
    "manifest_version": 2, 
    "permissions": [
        "tabs"
    ],  
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": ["content.js"]
        }
    ],
    "background": {
            "scripts": ["background.js"]
    },
    "web_accessible_resources": [
        "/nsfw.js"
    ]
}